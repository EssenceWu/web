<body>


<section id="choose-city" >
    <section class="bar-nav clearfix">
        <strong class="title">选择城市</strong>
    </section>

    <div class="city">
        <div class="city-list city-hot">
            <span class="city-letter">热门城市</span>
            {{#each hot_list}} 
                <p  data-curl='/index?sign={{../sign}}&city_id={{city_id}}&type={{../type}}&flush={{../flush}}&is_app={{../is_app}}' data-cityid="{{city_id}}">{{city_name}}</p>
            {{/each}}
        </div>
        <div class="city-list">
            {{#each first_list}} 
                <div class="city-list">
                    <span class="city-letter" id="{{letter}}1">{{letter}}</span>
                    {{#each first}} 
                        <p data-curl='/index?sign={{../../sign}}&city_id={{city_id}}&type={{../../type}}&flush={{../../flush}}&is_app={{../../is_app}}' data-cityid="{{city_id}}">{{city_name}}</p>
                    {{/each}}
                </div>
             {{/each}}
        </div>
        <div class="letter">
            <ul>
                <li><a href="javascript:;">A</a></li>
                <li><a href="javascript:;">B</a></li>
                <li><a href="javascript:;">C</a></li>
                <li><a href="javascript:;">D</a></li>
                <li><a href="javascript:;">E</a></li>
                <li><a href="javascript:;">F</a></li>
                <li><a href="javascript:;">G</a></li>
                <li><a href="javascript:;">H</a></li>
                <li><a href="javascript:;">J</a></li>
                <li><a href="javascript:;">K</a></li>
                <li><a href="javascript:;">L</a></li>
                <li><a href="javascript:;">M</a></li>
                <li><a href="javascript:;">N</a></li>
                <li><a href="javascript:;">O</a></li>
                <li><a href="javascript:;">P</a></li>
                <li><a href="javascript:;">Q</a></li>
                <li><a href="javascript:;">R</a></li>
                <li><a href="javascript:;">S</a></li>
                <li><a href="javascript:;">T</a></li>
                <li><a href="javascript:;">W</a></li>
                <li><a href="javascript:;">X</a></li>
                <li><a href="javascript:;">Y</a></li>
                <li><a href="javascript:;">Z</a></li>
            </ul>
        </div>
    </div>
</section>


<script src="/lookup/node_modules/jquery/dist/jquery.min.js"></script>
<script src="/lookup/scripts/base/global.js"></script>
<script src="/lookup/scripts/city.js?time={{time}}"></script>
</body>

</html>
