    <section class="bar-nav cMdGray clearfix {{#if is_app}}hide{{/if}}">
        <i class="iconfont icon-jiantou1 l goBack"></i>
        <strong class="title">公积金详情</strong>
    </section>
    <section class="shebao-detail-hd f24 hide" id="shebaoInfo" data-sign="{{sign}}" data-cityid="{{city_id}}" data-flush="{{flush}}" data-userid="{{userId}}">
        <div class="shebao-name-hd">
            <div>
                <div>
                    <h3>暂无数据</h3><span class="no-data">暂无数据</span><i class="no-data" id="update" data-searchId=""></i>
                </div>
                <div>
                    <i id="accountPage" data-isapp="{{is_app}}"></i>
                    <i id="addAccount"></i>
                </div>
            </div>
            <p>最后更新：<span>无</span></p>
        </div>
        <p class="no-data">账户：<span>无</span></p>
        <p class="no-data">余额：<span>无</span></p>
        <p class="no-data">累计缴纳：<span>无</span>个月</p>
        <p class="no-data">参保城市：<span>无</span></p>
        <p class="no-data">参保单位：<span>无</span></p>
    </section>
